{
  "name": "Bacon Ipsum Filler",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A simple tool to insert lorem ipsum text in an html form input.",
  "content_scripts": [
    {
      "matches": [
        "http://*",
        "https://*"
      ],
      "js": [
        "content-script.bundle.js"
      ]
    }
  ],
  "permissions": ["activeTab"]
}