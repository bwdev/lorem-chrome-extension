{
  "name": "Bacon Ipsum Filler",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A simple tool to insert lorem ipsum text in an html form input.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "pattern-matchers/pattern-matcher.js",
        "pattern-matchers/word-pattern-matcher.js",
        "pattern-matchers/lorem.pattern-matcher.js",
        "parse.js",
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "parse.js",
    "lorem.service.js"
 ]

}